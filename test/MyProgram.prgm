EXPORT average(list)
BEGIN
    LOCAL sum, count, i;
    sum := 0;
    count := DIM(list);
    FOR i FROM 1 TO count DO
        sum := sum + list(i);
    END;
    RETURN sum / count;
END;

EXPORT another()
